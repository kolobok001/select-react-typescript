{
  "name": "react-select",
  "version": "1.0.0",
  "description": "React select component",
  "main": "src/index.tsx",
  "style": "style/style.css",
  "repository": {
    "type": "git",
    "url": "git://github.com/fraserxu/react-dropdown.git"
  },
  "files": [
    "build/bundle.js",
    "src/index.tsx",
    "style/style.css"
  ],
  "keywords": [
    "react",
    "react-component",
    "component",
    "dropdown",
    "select"
  ],
  "license": "MIT",
  "dependencies": {
    "classnames": "^2.2.3"
  },
  "peerDependencies": {
    "react": "^0.14.7 || ^15.0.0-0 || ^16.0.0 || ^17.0.0",
    "react-dom": "^0.14.7 || ^15.0.0-0 || ^16.0.0 || ^17.0.0"
  },
  "browserify": {
    "transform": [
      "babelify"
    ]
  },
  "babel": {
    "presets": [
      "@babel/react",
      "@babel/env"
    ]
  },
  "devDependencies": {
    "@babel/cli": "^7.7.0",
    "@babel/core": "^7.7.2",
    "@babel/preset-env": "^7.7.1",
    "@babel/preset-react": "^7.7.0",
    "@types/react": "^17.0.2",
    "@types/react-dom": "^17.0.1",
    "babelify": "^7.2.0",
    "browserify": "^13.0.0",
    "browserify-hmr": "^0.3.1",
    "ecstatic": "^1.4.0",
    "gh-pages": "^0.11.0",
    "react": "^0.14.7 || ^15.0.0-0",
    "react-dom": "^0.14.7 || ^15.0.0-0",
    "standard": "^11.0.1",
    "watchify": "^3.7.0"
  },
  "scripts": {
    "build-dev": "webpack --config ./config/webpack.select.js --mode development --stats normal --color",
    "build-dev+live": "webpack --config ./config/webpack.select.js --mode development --stats normal --watch --color",
    "CSS-build": "node-sass style/style.scss -o style",
    "type-check": "tsc --noEmit",
    "type-check:watch": "npm run type-check -- --watch"
  }
}
